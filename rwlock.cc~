#include "rwlock.h"
#include <iostream>

RWLock:: RWLock(){
  pthread_mutex_init(&m_mutex,NULL);
}

pthread_mutex_t* RWLock:: getm_mutex(){
  return &m_mutex;
}

//RWLock:: startRead(){}

//RWLock:: doneRead(){}

//RWLock:: startWrite(){}

//RWLock:: doneWrite(){}

RWLock:: ~RWLock(){
  pthread_mutex_unlock(&m_mutex);
  pthread_mutex_destroy(&m_mutex);
}
